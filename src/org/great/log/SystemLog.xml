<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="org.great.log.SystemLogMapper">
 
<insert id="insert">

insert into log (id,DESCRIPTION,METHODS,LOGTYPE,REQUESTIP,EXCEPTIONCODE,EXCEPTIONDETAIL,PARAMS,CREATEBY,CREATEDATE) 
values    (#{id},#{description},#{method},#{logType},#{requestIp},#{exceptioncode},#{exceptionDetail},#{params},#{createBy},#{createDate}) 


</insert>
	 
</mapper>
